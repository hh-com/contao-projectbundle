{
    "type": "project",
	"repositories": [
        {
            "type": "path",
            "url": "src/hh-com/contao-projectbundle",
            "options": {
                    "symlink": true
            }
        }
    ],
    "require": {
        "contao/conflicts": "*@dev",
        "contao/manager-bundle": "5.6.*",
        "contao/calendar-bundle": "5.6.*",
        "contao/comments-bundle": "5.6.*",
        "contao/faq-bundle": "5.6.*",
        "contao/listing-bundle": "5.6.*",
        "contao/news-bundle": "5.6.*",
        "contao/newsletter-bundle": "5.6.*",
		"hh-com/contao-projectbundle": "@dev"
    },
    "extra": {
        "public-dir": "public",
        "contao-component-dir": "assets"
    },
    "scripts": {
        "post-install-cmd": [
            "@php vendor/bin/contao-setup"
        ],
        "post-update-cmd": [
            "@php vendor/bin/contao-setup"
        ]
    }
}